<?xml version="1.0" encoding="utf-8"?>

<TranscendenceModule>

	<Type UNID="&unidCCCabbageExpAttribs;">
		<AttributeDesc>
			<!-- Applies company name or generic label to items -->
			<ItemAttribute label="Cabbage Corp"			criteria="* +cabbageCorp;"/>
			
			<!-- This allows military crew members to have the appropriate tag, but not be confiscated/arrested at stations -->
			<ItemAttribute label="military"				labelType="advantage"	criteria="* +militaryCrewMember;"/>
			
			<!-- Things man was not meant to know -->
			<ItemAttribute label="political"			labelType="disadvantage"	criteria="* +cabbagePolitical;"/>
			
			<!-- Things man was not meant to touch -->
			<ItemAttribute label="WARNING: DO NOT ALLOW CONTACT WITH REACTOR FUELS!"			labelType="disadvantage"	criteria="* +protoCabbagium;"/>
		</AttributeDesc>
	</Type>

